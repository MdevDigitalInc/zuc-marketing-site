{
  "name": "pug-sass-templating-boilerplate",
  "description": "This project aims to provide a simple way of generating templated HTML5 / CSS pages",
  "version": "1.0.0",
  "author": "Lucas Moreira <lucas@mdev.digital>",
  "private": true,
  "main": "webpack.config.js",
  "scripts": {
    "dev": "webpack-dev-server --open --config webpack.dev.js",
    "lintcss": "stylelint '**/*.scss' --syntax scss",
    "prod": "webpack --config webpack.prod.js",
    "build": "npm run lintcss && npm run prod",
    "clean": "rm -r dist/"
  },
  "dependencies": {
